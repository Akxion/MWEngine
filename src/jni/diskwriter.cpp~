#include "diskwriter.h"

namespace DiskWriter
{
    std::string outputDir;

    void writeBufferToFile( double* aBuffer, int aFollowUpNum )
    {
FILE* file = fopen("/sdcard/hello.txt","w+");

    if (file != NULL)
    {
        fputs("HELLO WORLD!\n", file);
        fflush(file);
        fclose(file);
    }
    }
}
